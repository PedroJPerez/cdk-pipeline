    "cypress-multi-reporters": "^1.5.0",
    "jest": "^26.4.2",
    "junit-report-merger": "^3.0.5",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.8.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16",
